    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegoptim": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^8.0.0",
    "js-sha512": "^0.8.0",
    "livereload": "^0.9.1",
    "mime-types": "^2.1.28",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
